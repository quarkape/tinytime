page {
  font-size: 28rpx;
  height: 100%;
  color: white;
}
.nav-holder {
  /* position:fixed; */
  width:100%;
  top:0;
  left:0;
  z-index:2;
  background-color: #0081ff;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.nav {
  width: 50%;
  height: 60rpx;
  position: relative;
  display: inline-block;
  margin-right: 20rpx;
}
.nav-item {
  display: inline-block;
  text-align: center;
  width: 30%;
  height: 60rpx;
  color: white;
  line-height: 60rpx;
  font-size: 38rpx;
}
.ani-bar {
  position: absolute;
  width: 20%;
  bottom: 0;
  height: 32rpx;
  background-color: rgba(255, 255, 255, 0.4);
  border-radius: 16rpx;
  transition: all ease 0.3s;
}
.addOne {
  position: fixed;
  bottom: 60rpx;
  right: 60rpx;
  width: 100rpx;
  height: 100rpx;
  background-image: url("data:image/png;base64,");
  background-color: #0081ff;
  background-repeat: no-repeat;
  background-size: 40rpx 40rpx;
  background-position: center center;
  border-radius: 50%;
  z-index: 2;
  /* box-shadow: 0 0 20rpx #0081ff; */
}
.click-addOne {
  background-color: #0081ff;
}
.hot-info {
  width: 100%;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  margin: 10rpx 0;
  position: relative;
  color: #999;
  font-size: 24rpx;
}
.hot-info::after {
  content: "";
  position: absolute;
  left: 30%;
  width: 40%;
  z-index: -1;
  height:30rpx;
  top: 0;
}
.hot-info .txt {
  background-color: rgb(245, 247, 250);
  padding: 0 20rpx;
  display: inline;
}

.search {
  flex: 1;
  height: 80rpx;
  margin: 0 auto;
  margin-right: 20rpx;
  box-sizing: border-box;
  line-height: 80rpx;
  position: relative;
  display: inline-block;
  background-color: #fff;
  border-radius: 40rpx;
}
.search-input {
  height: 80rpx;
  top: 0;
  padding-left: 30rpx;
  outline: none;
  line-height: 80rpx;
  z-index: 0;
  box-sizing: border-box;
  color: #666;
  width: 70%;
}
.search-input-pstyle {
  color: #999;
  font-size: 28rpx;
}
.search-go {
  position: absolute;
  right: 0;
  top: 0;
  height: 80rpx;
  line-height: 80rpx;
  z-index: 1;
  color: #0081ff;
  width: 30%;
  text-align: center;
  background-image: url("data:image/svg+xml;base64,");
  background-size: 40rpx;
  background-repeat: no-repeat;
  background-position: center center;
}

.item {
  width: 92%;
  margin: 0 auto;
  position: relative;
  box-sizing: border-box;
  margin-top: 20rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.item-pic {
  position: absolute;
  width: 100%;
  border-radius: 10rpx;
  max-height: 300rpx;
  top: 0;
  left: 0;
}
.item-pic::before {
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
}
.item-title {
  position: relative;
  line-height: 40rpx;
  margin-top: 10rpx;
  padding: 0 10rpx 0 24rpx;
  margin-top: 10rpx;
}
.item-title::before {
  content: "";
  position: absolute;
  width: 8rpx;
  height: 100%;
  top: 0;
  left: 10rpx;
  background-color: white;
  border-radius: 4rpx;
}
.item-content-box {
  position: relative;
  width: 100%;
  margin-top: 20rpx;
}
.item-content {
  position: relative;
  width: 100%;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.item-data {
  position: relative;
  font-size: 24rpx;
  text-align: right;
  padding-right: 20rpx;
  margin: 10rpx 0 10rpx 0;
  overflow: hidden;
}
.bottom-loading {
  color: #666;
  height: 20px;
  width: 90%;
  margin: 0 auto;
  line-height: 20px;
  text-align: center;
}
.logotxt {
  width: 100%;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  margin: 10rpx 0;
  position: relative;
  color: #666;
}
.logotxt::after {
  content: "";
  position: absolute;
  left: 30%;
  width: 40%;
  z-index: -1;
  height:30rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.4);
  top: 0;
}
.logotxt .txt {
  background-color: rgb(245, 247, 250);
  padding: 0 20rpx;
  display: inline;
}

/* sla弹窗 */
.newSelect {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1110;
  opacity: 0;
  outline: 0;
  text-align: center;
  -ms-transform: scale(1.185);
  transform: scale(1.185);
  backface-visibility: hidden;
  perspective: 1998rpx;
  background: rgba(0, 0, 0, 0.6);
  transition: all 0.3s ease-in-out 0s;
  pointer-events: none;
}
.newSelect::before {
  content: "";
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
.newSelect.newSelectBottom {
  margin-bottom: -1000rpx;
}
.newSelect.newSelectBottom::before {
  vertical-align: bottom;
}
.newSelect.newSelectBottom.show {
  margin-bottom: 0;
}
.detail {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  max-width: 100%;
  background-color: #f8f8f8;
  overflow: hidden;
}
.closebtn {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  right: 30rpx;
  top: 10rpx;
  background-image: url("data:image/png;base64,");
  background-size: 30rpx 30rpx;
  background-repeat: no-repeat;
  background-position: center center;
}
.title {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #0081ff;
  color: white;
}
.options {
  width: 100%;
  height: 200rpx;
  color: #333;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.options > view {
  width: 30%;
  height: 200rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.options > view image {
  width: 80rpx;
  height: 80rpx;
}
.options > view > view {
  margin-top: 10rpx;
}
.show {
  opacity: 1;
  transition-duration: 0.3s;
  -ms-transform: scale(1);
  transform: scale(1);
  overflow-x: hidden;
  overflow-y: auto;
  pointer-events: auto;
  -webkit-overflow-scrolling: touch
}
.click-select {
  background-color: #EEEEEE;
}
