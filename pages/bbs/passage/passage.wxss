  height: 60rpx;
  bottom: -40rpx;
  left: 100rpx;
  border-radius: 15rpx;
  z-index: 1;
  box-shadow: 0 0 10rpx 2rpx rgba(0,0,0,0.08);
}
.p-detail::after {
